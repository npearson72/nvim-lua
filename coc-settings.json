{
  "coc": {
    "preferences": {
      "formatOnSave": true,
      "jumpCommand": "tabe",
      "willSaveHandlerTimeout": 5000
    },
    "source.emoji.filetypes": null
  },
  "diagnostic": {
    "errorSign": "",
    "warningSign": "",
    "infoSign": "",
    "hintSign": ""
  },
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "eruby": "html"
  },
  "emmet.showSuggestionsAsSnippets": true,
  "eslint.run": "onType",
  "explorer": {
    "file": {
      "reveal.auto": false,
      "child.template": "[git | 2][selection | clip | 1][indent][icon | 1] [diagnosticError & 1][filename omitCenter 1] [readonly & 1][linkIcon & 1][link]",
      "root.template": "[icon] [fullpath]",
      "showHiddenFiles": true,
      "filename.colored.enable": {
        "diagnosticError": true,
        "diagnosticWarning": false,
        "git": false
      }
    },
    "git.showIgnored": false,
    "icon.enableNerdfont": true,
    "keyMappings.global": {
      "<2-LeftMouse>": "null",
      "<Leader>e": "reveal:select",
      "l": "null",
      "E": "null",
      "v": "open:vsplit"
    },
    "openAction": {
      "relativePath": true,
      "strategy": "sourceWindow"
    },
    "previewAction.onHover": false,
    "quitOnOpen": true,
    "sources": [
      {
        "name": "file",
        "expand": true
      }
    ]
  },
  "git": {
    "addedSign.hlGroup": "CocGitAdd",
    "changedSign.hlGroup": "CocGitChange",
    "removedSign.hlGroup": "CocGitDelete",
    "topRemovedSign.hlGroup": "CocGitDelete",
    "changeRemovedSign.hlGroup": "CocGitDeleteChange",
    "changedSign.text": "!"
  },
  "javascript.suggestionActions.enabled": false,
  "Lua.runtime.version": "LuaJIT",
  "Lua.workspace.checkThirdParty": false,
  "Lua.hint.enable": false,
  "Lua.telemetry.enable": false,
  "prettier": {
    "arrowParens": "avoid",
    "endOfLine": "auto",
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "snippets.extends": {
    "typescriptreact": ["javascript"],
    "typescript": ["javascript"]
  },
  "solargraph": {
    "diagnostics": true,
    "formatting": true
  },
  "suggest": {
    "snippetIndicator": " ",
    "maxCompleteItemCount": 500,
    "noselect": true
  },
  "sumneko-lua.enableNvimLuaDev": true,
  "vetur.validation.template": false,
  "workspace.rootPatterns": ["package.json", ".gitignore", ".env", ".git"]
}
